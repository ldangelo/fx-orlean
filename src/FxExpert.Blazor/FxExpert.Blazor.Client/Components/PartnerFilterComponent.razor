                         Variant="Variant.Text"
                         OnClick="ClearAllFilters"
                         StartIcon="@Icons.Material.Filled.Clear"
                         Class="mt-2">
                    Clear All Filters
                </MudButton>
            </MudPaper>
        }
        
        <!-- Filter Panels -->
        <MudExpansionPanels>
            <!-- Location Filter -->
            <MudExpansionPanel Text="üìç Location" IsInitiallyExpanded="@(_activeLocationFilters > 0)">
                <MudGrid>
                    <MudItem xs="12" md="6">
                        <MudAutocomplete T="string" 
                                       Label="City" 
                                       SearchFunc="SearchCities"
