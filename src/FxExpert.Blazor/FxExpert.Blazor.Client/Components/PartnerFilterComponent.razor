                </MudGrid>
            </MudExpansionPanel>
            
            <!-- Availability Filter -->
            <MudExpansionPanel Text="üìÖ Availability" IsInitiallyExpanded="@(_selectedAvailability.HasValue)">
                <MudRadioGroup T="AvailabilityTimeframe?" 
