{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak": {
    "realm": "fx-expert",
    "auth-server-url": "http://localhost:8085/",
    "SslRequired": false,
    "ssl-required": "none",
    "resource": "fx-expert",
    "verify-token-audience": false,
    "use-resource-role-mappings": true,
    "confidential-port": 0
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "AllowedHosts": "*"
}
